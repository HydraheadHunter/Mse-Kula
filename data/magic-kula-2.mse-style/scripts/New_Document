
switch_symbol_pos		:=	{(	if		chosen(choice:"leveler",		input)			then		leveler			
							else if	chosen(choice:"adventure",	input)			then		adventure			
							else if	chosen(choice:"kami flip",	input)			then		kamigawa_flip		
							else if	chosen(choice:"aftermath",	input)			then		aftermath			
							else if	chosen(choice:"split",		input)			then		split			
							else if	chosen(choice:"saga",		input)			then		saga				
							else if	chosen(choice:"planeswalker",	input)			then		planeswalker		
							else														default		)	.symbol
					}

